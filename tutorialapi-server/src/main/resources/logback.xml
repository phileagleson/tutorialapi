<configuration>
        <appender name="STDOUT"
        class="ch.qos.logback.core.ConsoleAppender">
                <encoder>
                        <pattern>%d %-4L 5level %-50logger{50} -
                        %msg%n</pattern>
                </encoder>
        </appender>
        <root level="INFO">
                <appender-ref ref="STDOUT" />
        </root>
        <logger name="com.tutorialapi" level="INFO" />
</configuration>
